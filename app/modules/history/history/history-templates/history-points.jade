.diff-points-wrapper(ng-repeat="(key, diff) in activity.values_diff.points")
    span.key(
        translate="ACTIVITY.US_POINTS"
        translate-values="{role: key}"
    )
    span.diff {{diff[0]}}
    tg-svg.comment-option(
        svg-icon="icon-arrow-right"
        svg-title-translate="COMMON.EDIT"
    )
    span.diff {{diff[1]}}
